{
  "name": "virtual-hero",
  "version": "1.0.0",
  "description": "App móvil de fitness gamificada con estética de cómic y superhéroes",
  "main": "expo-router/entry",
  "scripts": {
    "dev": "npx expo start",
    "start": "npx expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "prebuild": "expo prebuild",
    "prebuild:clean": "expo prebuild --clean",
    "build:dev": "eas build --platform android --profile development",
    "build:dev:local": "eas build --platform android --profile development --local",
    "build:preview": "eas build --platform android --profile preview",
    "build:preview:ios": "eas build --platform ios --profile preview",
    "build:prod": "eas build --platform all --profile production",
    "build:prod:android": "eas build --platform android --profile production",
    "build:prod:ios": "eas build --platform ios --profile production",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "postinstall": "patch-package",
    "prepare": "husky install"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-navigation/native": "^7.1.8",
    "@tanstack/react-query": "^5.8.4",
    "expo": "^54.0.21",
    "expo-audio": "~1.0.14",
    "expo-constants": "~18.0.10",
    "expo-dev-client": "~6.0.17",
    "expo-device": "~8.0.9",
    "expo-font": "~14.0.9",
    "expo-haptics": "~15.0.7",
    "expo-linear-gradient": "~15.0.7",
    "expo-linking": "~8.0.8",
    "expo-notifications": "~0.32.12",
    "expo-router": "~6.0.14",
    "expo-sensors": "~15.0.7",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "lottie-react-native": "~7.3.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-markdown-display": "^7.0.2",
    "react-native-mmkv": "^2.12.2",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-worklets": "0.5.1",
    "victory-native": "^36.9.2",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "@types/react": "~19.1.10",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^8.55.0",
    "eslint-config-expo": "~10.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "patch-package": "^8.0.1",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^3.1.1",
    "typescript": "^5.3.3"
  },
  "private": true,
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "react-native-google-fit"
        ]
      }
    }
  }
}
